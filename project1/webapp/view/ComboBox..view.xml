<mvc:View
	controllerName="project1.controller.ComboBox"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
    displayBlock="true">
	<Page showHeader="false" class="sapUiContentPadding">
		<content>
			<ComboBox
                           items="{
                              path: '/results',
                              sorter: { path: 'SubprojectTypes' }
                           }">
                           <core:Item  key="{SubprojectTypes}" text="{SubprojectTypes}"  />
                        </ComboBox>
		</content>
	</Page>
</mvc:View>